<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <stroke android:width="1dp" android:color="#968de6"/>
            <solid android:color="#968de6"></solid>
            <corners
                android:radius="20dp" />

        </shape>
   </item>

    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#ada4ee"/>
            <solid android:color="#ada4ee"></solid>
            <corners
                android:radius="20dp"/>
        </shape>
    </item>
</selector>
